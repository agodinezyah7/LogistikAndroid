<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffafafa">
    <ImageView
        android:src="@drawable/logo_logistik"
        android:layout_width="match_parent"
        android:layout_height="92dp"
        android:id="@+id/imageView1"
        android:layout_marginTop="10dp" />
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout1"
        android:layout_marginTop="10dp"
        android:gravity="center">
        <Button
            android:id="@+id/ButtonCompras"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="compras"
            android:layout_marginRight="20dp"
            android:background="#ff2196f3"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"/>
        <Button
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:id="@+id/ButtonUbicarProd"
            android:layout_toRightOf="@id/ButtonCompras"
            android:text="Ubicar prod."
            android:background="#ff2196f3"
            android:paddingTop="15dp"
            android:paddingBottom="15dp" />

         <Button
            android:id="@+id/ButtonPedido"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="Surtir pedido"
            android:layout_below="@id/ButtonCompras"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:background="#ff2196f3"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"/>

         <Button
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:id="@+id/ButtonReubicar"
            android:layout_toRightOf="@id/ButtonPedido"
            android:layout_below="@id/ButtonUbicarProd"
            android:layout_marginTop="20dp"
            android:text="Reubicar"
            android:background="#ff2196f3"
            android:paddingTop="15dp"
            android:paddingBottom="15dp" />

         <Button
            android:id="@+id/ButtonConsulta"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="Consulta"
            android:layout_below="@id/ButtonPedido"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:background="#ff2196f3"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"/>
         <Button
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:id="@+id/ButtonEmpaques"
            android:layout_toRightOf="@id/ButtonConsulta"
            android:layout_below="@id/ButtonReubicar"
            android:layout_marginTop="20dp"
            android:text="Empaques"
            android:background="#ff2196f3"
            android:paddingTop="15dp"
            android:paddingBottom="15dp" />
           <Button
            android:id="@+id/Buttonentrada"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="entrada"
            android:layout_below="@id/ButtonConsulta"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:background="#ff2196f3"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"/>
         <Button
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:id="@+id/ButtonSalida"
            android:layout_toRightOf="@id/Buttonentrada"
            android:layout_below="@id/ButtonEmpaques"
            android:layout_marginTop="20dp"
            android:text="Salida"
            android:background="#ff2196f3"
            android:paddingTop="15dp"
            android:paddingBottom="15dp" />
        <Button
            android:id="@+id/ButtonInventario"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="Inventario"
            android:layout_below="@+id/Buttonentrada"
            android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:background="#ff2196f3"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"/>
         <Button
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:id="@+id/ButtonConfiguracion"
            android:layout_toRightOf="@id/ButtonInventario"
            android:layout_below="@id/ButtonSalida"
            android:layout_marginTop="20dp"
            android:text="Configuracion"
            android:background="#ff2196f3"
            android:paddingTop="15dp"
            android:paddingBottom="15dp" />

    </RelativeLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:id="@+id/relativeLayout4">
        <TextView
            android:text="Bateria:"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBateria"
            android:layout_marginLeft="10dp"
            android:textColor="#ff212121"
            android:textStyle="bold" />
        <TextView
            android:text="100%"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewBateriaP"
            android:textColor="#ff212121"
            android:layout_toRightOf="@id/textViewBateria"
            android:layout_alignBottom="@id/textViewBateria"
            android:textStyle="bold" />
        <TextView
            android:text="Wifi:"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/textViewWifi"
            android:layout_marginLeft="10dp"
            android:textColor="#ff212121"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:layout_below="@id/textViewBateria" />
        <ImageView
            android:src="@drawable/wifigreen"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/imageViewWifi"
            android:layout_below="@id/textViewBateriaP"
            android:layout_toRightOf="@id/textViewWifi" />
        <Button
            android:id="@+id/ButtonSalir"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="salir"
            android:layout_marginLeft="200dp"
            android:background="#ff2196f3"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"/>
    </RelativeLayout>
        <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:id="@+id/relativeLayout5">
        <TextView
            android:layout_width="150.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:text="Ver.1.00"
            android:textColor="#ff303f9f"
            android:textSize="30px"
            android:layout_marginRight="20dp"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"/>
        <ImageView
            android:src="@drawable/logobildden"
            android:layout_width="170dp"
            android:layout_height="60dp"
            android:id="@+id/imageView2"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            android:layout_toRightOf="@+id/textView2"
            />
    </RelativeLayout>
 
</LinearLayout>
